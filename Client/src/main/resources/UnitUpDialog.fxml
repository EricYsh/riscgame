<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.duke.ece651.riscgame.UnitUpDialogController" style="-fx-background-image: url('/images/bg3.png'); -fx-background-size: cover;" stylesheets="/stylesheet/risc.css">
   <center>
      <AnchorPane prefHeight="394.0" prefWidth="532.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="territory_in" layoutX="66.0" layoutY="66.0" prefHeight="50.0" prefWidth="170.0" />
            <TextField fx:id="units_index" layoutX="66.0" layoutY="208.0" prefHeight="50.0" prefWidth="170.0" />
            <TextField fx:id="level_to" layoutX="66.0" layoutY="341.0" prefHeight="50.0" prefWidth="170.0" />
            <Label layoutX="57.0" layoutY="36.0" text="The territory you want to upgrade units in:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="49.0" layoutY="144.0" text=" The index of units you want to upgrade">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="55.0" layoutY="171.0" text="(separate by a space): e.g. &quot;1 2 3&quot;">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="57.0" layoutY="281.0" text="Which level you want to upgrade to:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="58.0" layoutY="306.0" text="(seperate by a space, between 0 - 6)">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="cancel_btn" layoutX="411.0" layoutY="112.0" mnemonicParsing="false" onAction="#click_cancel" prefHeight="48.0" prefWidth="158.0" text="Cancel" />
            <Button fx:id="up_unit_btn" layoutX="411.0" layoutY="227.0" mnemonicParsing="false" onAction="#click_up_unit" prefHeight="48.0" prefWidth="159.0" text="Up Unit" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
